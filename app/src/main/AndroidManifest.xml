<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cxh.note">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ListItemAnimationActivity" />
        <activity android:name=".activity.LazyLoadViewPagerActivity" />
        <activity android:name=".activity.LazyLoadDataPreLoadViewActivity" />
        <activity android:name=".activity.CircleProgressViewActivity" />
        <activity android:name=".activity.TestActivity" />
    </application>

</manifest>